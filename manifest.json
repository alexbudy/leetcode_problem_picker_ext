{
  "name": "Leetcode Problem Picker",
  "description": "Pick Leetcode Problems via extension",
  "version": "1.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/lcpp16.png",
      "32": "/images/lcpp32.png",
      "48": "/images/lcpp48.png",
      "128": "/images/lcpp128.png"
    }
  },
  "icons": {
    "16": "/images/lcpp16.png",
    "32": "/images/lcpp32.png",
    "48": "/images/lcpp48.png",
    "128": "/images/lcpp128.png"
  }
}
